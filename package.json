{
  "name": "swolmodoro",
  "version": "1.1.0",
  "private": true,
  "description": "A pomodoro timer that prompts you to exercise in-between breaks.",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:content": "contentlayer build",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "db:migrate": "run-p db:migrate:*",
    "db:migrate:dev": "npx prisma migrate dev",
    "db:migrate:test": "dotenv -e .env.test -- npx prisma db push --accept-data-loss",
    "db:up": "docker-compose up -d",
    "db:down": "docker-compose down",
    "db:clear": "rm -r postgres-data",
    "db:seed": "npx prisma db seed",
    "db:setup": "run-s db:up db:migrate",
    "db:teardown": "run-s db:down db:clear",
    "db:studio": "npx prisma studio -p 7777 --browser none",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:content": "contentlayer dev",
    "dev:remix": "cross-env NODE_ENV=development netlify dev",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "npm run postinstall",
    "start": "cross-env NODE_ENV=production netlify dev -p 8811",
    "test": "run-s db:migrate:test test:unit:ci test:integration:ci",
    "test:integration": "dotenv -e .env.test -- vitest --no-threads --dir ./test/routes",
    "test:integration:ci": "npm run test:integration -- --run",
    "test:unit": "dotenv -e .env.test -- vitest --dir ./app",
    "test:unit:ci": "npm run test:unit -- --run",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 'cypress open'",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env start-server-and-test http://localhost:8811 'cypress run'",
    "typecheck": "tsc -b && tsc -b cypress && tsc -b test/e2e",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "storybook": "start-storybook -p 6006 --quiet --no-open",
    "build-storybook": "build-storybook"
  },
  "prettier": {},
  "eslintIgnore": [
    "/netlify/functions/server",
    "/node_modules",
    "/public/build",
    "/server/build",
    "/build"
  ],
  "dependencies": {
    "@headlessui/react": "^1.6.1",
    "@heroicons/react": "^1.0.6",
    "@netlify/functions": "^0.10.0",
    "@node-rs/bcrypt": "^1.6.0",
    "@prisma/client": "^3.11.0",
    "@reach/alert": "^0.16.0",
    "@remix-run/netlify": "^1.4.3",
    "@remix-run/node": "^1.4.3",
    "@remix-run/react": "^1.4.3",
    "@remix-run/serve": "^1.4.3",
    "@supabase/supabase-js": "^1.35.2",
    "contentlayer": "^0.2.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@faker-js/faker": "^6.0.0",
    "@remix-run/dev": "^1.4.3",
    "@remix-run/eslint-config": "^1.4.3",
    "@storybook/addon-actions": "^6.4.21",
    "@storybook/addon-essentials": "^6.4.21",
    "@storybook/addon-interactions": "^6.4.21",
    "@storybook/addon-links": "^6.4.21",
    "@storybook/react": "^6.4.21",
    "@storybook/testing-library": "^0.0.9",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/eslint": "^8.4.1",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@vitejs/plugin-react": "^1.2.0",
    "babel-loader": "^8.2.4",
    "binode": "^1.0.5",
    "c8": "^7.11.0",
    "cross-env": "^7.0.3",
    "cypress": "^9.5.2",
    "esbuild-register": "^3.3.2",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "happy-dom": "^2.49.0",
    "msw": "^0.39.2",
    "npm-run-all": "^4.1.5",
    "prettier": "2.6.0",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "prisma": "^3.11.0",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.10.0"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  }
}
